<DialogPane minHeight="250.0" minWidth="400.0" prefHeight="250.0" prefWidth="400.0" snapToPixel="false" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.controllers.Sprint.SprintWindowController">
   <buttonTypes>
      <ButtonType fx:constant="APPLY" />
      <ButtonType fx:constant="CANCEL" />
   </buttonTypes>
   <content>
      <VBox prefHeight="256.0" prefWidth="400.0">
         <children>
            <GridPane prefHeight="263.0" prefWidth="378.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="183.0" minWidth="10.0" prefWidth="133.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="289.0" minWidth="10.0" prefWidth="245.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="Название" />
                  <Label text="Дата начала" GridPane.rowIndex="1" />
                  <Label text="Дата окончания" GridPane.rowIndex="2" />
                  <TextField fx:id="title" prefHeight="65.0" prefWidth="289.0" GridPane.columnIndex="1" />
                  <DatePicker fx:id="startDate" prefHeight="24.0" prefWidth="250.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <DatePicker fx:id="endDate" prefHeight="24.0" prefWidth="249.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
               </children>
            </GridPane>
         </children>
      </VBox>
   </content>
</DialogPane>
